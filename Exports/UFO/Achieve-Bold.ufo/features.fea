} case;

feature dlig {
# automatic
lookupflag IgnoreMarks;
sub A S by A_S;
sub E S by E_S;
sub I J by I_J;
sub K S by K_S;
sub N N by N_N;
sub R S by R_S;
} dlig;

feature liga {
# automatic
lookupflag IgnoreMarks;
sub f i by fi;
sub f l by fl;
} liga;

feature cpsp {
# automatic
pos @Uppercase <5 0 10 0>;
} cpsp;

